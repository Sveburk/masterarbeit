#-------------Document_Properties--------------

:hasAuthorID a rdfs:Property ;                  # Gibt an, wer das Dokument verfasst hat (Person oder Organisation).
    rdfs:label "has author"@en ;
    rdfs:label "hat Verfasser"@de ;
    rdfs:comment "The person or organization that created the document."@en ;
    rdfs:comment "Die Person oder Organisation, die das Dokument erstellt hat."@de ;
    rdfs:domain :Document ;
    rdfs:range [ a owl:Class ; owl:unionOf (:Person :Organization) ] .

:hasDocumentID a rdfs:Property ;              # Eindeutige ID, die jedes Dokument identifiziert.
    rdfs:label "has document ID"@en ;
    rdfs:label "hat Dokumenten-ID"@de ;
    rdfs:comment "A unique identifier for the document."@en ;
    rdfs:comment "Eine eindeutige Kennung für das Dokument."@de ;
    rdfs:domain :Document ;
    rdfs:range xsd:string .

:hasRecipientID a rdfs:Property ;               # Beschreibt, an wen das Dokument gerichtet ist (Person oder Organisation).
    rdfs:label "has recipient"@en ;
    rdfs:label "hat Empfänger"@de ;
    rdfs:comment "The person or organization to whom the document is addressed."@en ;
    rdfs:comment "Die Person oder Organisation, an die das Dokument gerichtet ist."@de ;
    rdfs:domain :Document ;
    rdfs:range [ a owl:Class ; owl:unionOf (:Person :Organization) ] .

:hasTranscription a rdfs:Property ;          # Datum, an dem das Dokument transkribiert wurde.
    rdfs:label "has the transcription"@en ;
    rdfs:label "hat die Transkription"@de ;
    rdfs:comment "refers to a transcription."@en ;
    rdfs:comment "Referenziert auf eine Transkription."@de ;
    rdfs:domain :Document ;
    rdfs:range xsd:string .

:hasCreationPlace a rdfs:Property ;              # Ort, an dem das Dokument erstellt wurde.
    rdfs:label "creation place"@en ;
    rdfs:label "Entstehungsort"@de ;
    rdfs:comment "The place where the document was created."@en ;
    rdfs:comment "Der Ort, an dem das Dokument erstellt wurde."@de ;
    rdfs:domain :Document ;
    rdfs:range schema:Place .

:hasReceiptPlace a rdfs:Property ;               # Ort, an dem das Dokument empfangen wurde.
    rdfs:label "receipt place"@en ;
    rdfs:label "Empfangsort"@de ;
    rdfs:comment "The place where the document was received."@en ;
    rdfs:comment "Der Ort, an dem das Dokument empfangen wurde."@de ;
    rdfs:domain :Document ;
    rdfs:range schema:Place .

:hasContentTags a rdfs:Property ;                # Tags, die den Inhalt des Dokuments beschreiben.
    rdfs:label "content tags"@en ;
    rdfs:label "Inhalt-Tags"@de ;
    rdfs:comment "Tags that describe the content or themes of the document."@en ;
    rdfs:comment "Tags, die den Inhalt oder die Themen des Dokuments beschreiben."@de ;
    rdfs:domain :Document ;
    rdfs:range xsd:string .

:hasCreationDate a rdfs:Property ;               # Das Datum, an dem das Dokument ursprünglich erstellt wurde.
    rdfs:label "creation date"@en ;
    rdfs:label "Entstehungsdatum"@de ;
    rdfs:comment "The date when the document was originally created."@en ;
    rdfs:comment "Das Datum, an dem das Dokument ursprünglich erstellt wurde."@de ;
    rdfs:domain :Document ;
     rdfs:range [
        a owl:Class ;
        owl:unionOf (xsd:date xsd:gYear)
    ] .


