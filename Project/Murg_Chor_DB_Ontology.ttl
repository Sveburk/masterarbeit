#--------------Documenten-Classes------------
#////////////////////////////////////////////


:Document a rdfs:Class ;
    rdfs:subClassOf schema:CreativeWork ;
    rdfs:label "Document"@en ;
    rdfs:label "Dokument"@de ;
    rdfs:comment "A document, typically a written or printed work, that can be formal or informal."@en ;
    rdfs:comment "Ein Dokument, typischerweise ein schriftliches oder gedrucktes Werk, das formal oder informell sein kann."@de ;
    owl:sameAs wd:Q49848 .


#--------------Documenten-Sub-Classes------------

:Letter a rdfs:Class ;                     #Brief
    rdfs:subClassOf :Document; 
    rdfs:label "Letter"@en ;
    rdfs:label "Brief"@de ;
    rdfs:comment "A written message, typically sent from one person to another."@en ;
    rdfs:comment "Ein schriftliches Dokument, das typischerweise von einer Person an eine andere gesendet wird."@de ;
    owl:sameAs wd:Q133492 .

:report a rdfs:Class ;                      #Protocol
    rdfs:subClassOf :Document ;
    rdfs:label "report"@en ;
    rdfs:label "Protokoll"@de ;
    rdfs:comment "A record or minutes of a meeting or event."@en ;
    rdfs:comment "Ein Protokoll oder eine Aufzeichnung von einer Sitzung oder einem Ereignis."@de ;
    owl:sameAs wd:Q10870555 .

:Postcard a rdfs:Class ;                    #Postkarte
    rdfs:subClassOf :Document ;
    rdfs:label "Postcard"@en ;
    rdfs:label "Postkarte"@de ;
    rdfs:comment "A postcard, typically used for sending short messages with or without an envelope."@en ;
    rdfs:comment "Eine Postkarte, die typischerweise für das Versenden von kurzen Nachrichten mit oder ohne Umschlag verwendet wird."@de ;
    owl:sameAs wd:Q192425 .

:Invoice a rdfs:Class ;                     #Rechnung
    rdfs:subClassOf :Document ;
    rdfs:label "Invoice"@en ;
    rdfs:label "Rechnung"@de ;
    rdfs:comment "A document that details a transaction, including the goods or services provided and the payment terms."@en ;
    rdfs:comment "Ein Dokument, das eine Transaktion beschreibt, einschließlich der gelieferten Waren oder Dienstleistungen und der Zahlungsbedingungen."@de ;
    owl:sameAs wd:Q190581 .

:GovernmentDocument a rdfs:Class ;          #Behördenpost/Regierungsdokument
    rdfs:subClassOf :Document ;
    rdfs:label "Government document"@en ;
    rdfs:label "Regierungsdokument"@de ;
    rdfs:comment "A document issued by a government entity, typically containing official information or records."@en ;
    rdfs:comment "Ein Dokument, das von einer Regierungsstelle herausgegeben wurde und in der Regel offizielle Informationen oder Aufzeichnungen enthält."@de ;
    owl:sameAs wd:Q30941709 .

:Map a rdfs:Class ;                         #Karte (geographisch)
    rdfs:subClassOf :Document ;
    rdfs:label "Map"@en ;
    rdfs:label "Karte"@de ;
    rdfs:comment "A visual representation of an area, often used for navigation or spatial understanding."@en ;
    rdfs:comment "Eine visuelle Darstellung eines Gebiets, die häufig zur Navigation oder zum räumlichen Verständnis verwendet wird."@de ;
    owl:sameAs wd:Q4006 .

:SheetMusic a rdfs:Class ;                  #Noten/Notenblatt
    rdfs:subClassOf :Document ;
    rdfs:label "Sheet music"@en ;
    rdfs:label "Notenblatt"@de ;
    rdfs:comment "A musical score or written representation of musical notes, used for performing music."@en ;
    rdfs:comment "Eine musikalische Partitur oder schriftliche Darstellung von Noten, die zur Aufführung von Musik verwendet wird."@de ;
    owl:sameAs wd:P3030 .

:NewspaperArticle a rdfs:Class ;            #Zeitungsartikel
    rdfs:subClassOf :Document ;
    rdfs:label "Newspaper article"@en ;
    rdfs:label "Zeitungsartikel"@de ;
    rdfs:comment "An article published in a newspaper, typically covering news, opinions, or other subjects of public interest."@en ;
    rdfs:comment "Ein in einer Zeitung veröffentlichter Artikel, der in der Regel Nachrichten, Meinungen oder andere Themen von öffentlichem Interesse behandelt."@de ;
    owl:sameAs wd:Q5707594 .

:List a rdfs:Class ;                        #Liste
    rdfs:subClassOf :Document ;
    rdfs:label "List"@en ;
    rdfs:label "Liste"@de ;
    rdfs:comment "A collection of items, typically arranged in a specific order or according to a particular criteria."@en ;
    rdfs:comment "Eine Sammlung von Elementen, die typischerweise in einer bestimmten Reihenfolge oder nach bestimmten Kriterien angeordnet ist."@de ;
    owl:sameAs wd:Q12139612 .

:Website a rdfs:Class ;                    #Website
    rdfs:subClassOf :Document ;
    rdfs:label "Website"@en ;
    rdfs:label "Webseite"@de ;
    rdfs:comment "A collection of related web pages served from a single domain, often containing information, media, or other resources."@en ;
    rdfs:comment "Eine Sammlung von zusammenhängenden Webseiten, die von einer einzigen Domain bereitgestellt werden und häufig Informationen, Medien oder andere Ressourcen enthalten."@de ;
    owl:sameAs wd:Q35127 .

:TextAnnotation a rdfs:Class ;             #Notiz/Notizzettel
    rdfs:subClassOf :Document ;
    rdfs:label "Text annotation"@en ;
    rdfs:label "Notiz"@de ;
    rdfs:comment "A note or annotation added to a text, often used for clarification, comments, or additional information."@en ;
    rdfs:comment "Eine Notiz oder Anmerkung, die zu einem Text hinzugefügt wird und häufig zur Klärung, für Kommentare oder zusätzliche Informationen verwendet wird."@de ;
    owl:sameAs wd:Q2851776 .

:Offer a rdfs:Class ;                      #Offerte/Angebot   
    rdfs:subClassOf :Document ;
    rdfs:label "Offer"@en ;
    rdfs:label "Offerte"@de ; 
    rdfs:comment "A proposal or quotation provided, often in a commercial context, outlining the terms and conditions for a service or product."@en ;
    rdfs:comment "Ein Vorschlag oder Angebot, oft im kommerziellen Kontext, der die Bedingungen und Konditionen für eine Dienstleistung oder ein Produkt beschreibt."@de ;
    owl:sameAs wd:Q6501319 .





