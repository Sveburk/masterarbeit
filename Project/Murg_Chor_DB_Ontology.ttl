#_Personen__

# Beispiel einer Person mit den definierten Attributen

#:person1234 a :Person ;
    #schema:familyName "Mustermann" ;
    #schema:givenName "Suter" ;
    #schema:alternateName "Manni" ;
    #schema:homeLocation wd:Q64;                        #wikidata für Berlin
    #schema:birthDate "1920-05-15"^^xsd:date ;
